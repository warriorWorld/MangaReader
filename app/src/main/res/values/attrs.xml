<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="home_top_bar">
        <attr name="topbar_background" format="reference|color"></attr>
        <attr name="left_press_background" format="reference|color"></attr>
        <attr name="left_background" format="reference|color"></attr>
        <attr name="left_textSize" format="dimension|reference"></attr>
        <attr name="left_text" format="string|reference"></attr>
        <attr name="left_textColor" format="color" />
        <attr name="right_background" format="reference|color"></attr>
        <attr name="right_textSize" format="dimension|reference"></attr>
        <attr name="right_text" format="string|reference"></attr>
        <attr name="right_textColor" format="color" />
        <attr name="title_text" format="string|reference"></attr>
        <attr name="title_textSize" format="dimension|reference"></attr>
        <attr name="title_textColor" format="color" />
        <attr name="title_src" format="reference|color"></attr>
    </declare-styleable>

    <declare-styleable name="user_top_bar">
        <attr name="user_topbar_background" format="reference|color"></attr>
        <attr name="left1_press_background" format="reference|color"></attr>
        <attr name="left1_background" format="reference|color"></attr>
        <attr name="left1_textSize" format="dimension|reference"></attr>
        <attr name="left1_text" format="string|reference"></attr>
        <attr name="left1_textColor" format="color" />
        <attr name="left2_press_background" format="reference|color"></attr>
        <attr name="left2_background" format="reference|color"></attr>
        <attr name="left2_textSize" format="dimension|reference"></attr>
        <attr name="left2_text" format="string|reference"></attr>
        <attr name="left2_textColor" format="color" />
        <attr name="left3_press_background" format="reference|color"></attr>
        <attr name="left3_background" format="reference|color"></attr>
        <attr name="left3_textSize" format="dimension|reference"></attr>
        <attr name="left3_text" format="string|reference"></attr>
        <attr name="left3_textColor" format="color" />
        <attr name="right1_background" format="reference|color"></attr>
        <attr name="right1_textSize" format="dimension|reference"></attr>
        <attr name="right1_text" format="string|reference"></attr>
        <attr name="right1_textColor" format="color" />
        <attr name="right2_background" format="reference|color"></attr>
        <attr name="right2_textSize" format="dimension|reference"></attr>
        <attr name="right2_text" format="string|reference"></attr>
        <attr name="right2_textColor" format="color" />
    </declare-styleable>
    <declare-styleable name="RoundProgressBar">
        <attr name="roundColor" format="color" />
        <attr name="roundProgressColor" format="color" />
        <attr name="roundWidth" format="dimension"></attr>
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="max" format="integer"></attr>
        <attr name="textIsDisplayable" format="boolean"></attr>
        <attr name="style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>
    <attr name="color_no_finger_inner_circle" format="color" />
    <attr name="color_no_finger_outer_circle" format="color" />
    <attr name="color_finger_on" format="color" />
    <attr name="color_finger_up" format="color" />
    <attr name="count" format="integer" />
    <attr name="tryTimes" format="integer" />
    <declare-styleable name="GestureLockViewGroup">
        <attr name="color_no_finger_inner_circle" />
        <attr name="color_no_finger_outer_circle" />
        <attr name="color_finger_on" />
        <attr name="color_finger_up" />
        <attr name="count" />
        <attr name="tryTimes" />
    </declare-styleable>
    <declare-styleable name="max_height_scrollview">
        <attr name="max_height" format="dimension" />
    </declare-styleable>
</resources>